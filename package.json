{
  "name": "brickbot-ui",
  "version": "0.0.1",
  "description": "This is the official BrickBot FTC Team component library, to be used across all of its web platforms.",
  "keywords": [
    "react",
    "components",
    "brickbot"
  ],
  "homepage": "https://github.com/BrickBot15996/brickbot-ui#readme",
  "bugs": {
    "url": "https://github.com/BrickBot15996/brickbot-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrickBot15996/brickbot-ui.git"
  },
  "license": "MIT",
  "author": "Marius Baican",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "node scripts/generate-index.js",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\"",
    "build:types": "tsc",
    "build:css": "npx @tailwindcss/cli -i index.css -o dist/index.css",
    "build:copy": "copyfiles -f README.md LICENSE dist",
    "build": "npm run generate && npm run build:js && npm run build:types && npm run build:css && npm run build:copy && cd .\\test\\playground\\ && npm install ..\\..\\"
  },
  "dependencies": {
    "react": "^19.2.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "index.js",
    "index.css"
  ],
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@tailwindcss/cli": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
